{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "qoutes": ["error", "always"],
        "no-class-asign": ["error", "always"],
        "no-cond-assign": ["error", "always"],
        "no-constant-condition": ["error", "always"],
        "no-dupe-keys": ["error", "always"]
    }
}
